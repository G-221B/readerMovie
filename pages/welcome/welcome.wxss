page {
  height: 100%;
  background-color: #9dc2ca;
}
.body {
  display: flex;
  flex-direction: column;
  align-items: center;
}
image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 60px;
  margin-bottom: 30px;
  border-radius: 50%;
}
text {
	font-weight: bold;
}
.button {
  margin-top: 30px;
  font-size: 10px;
  padding: 5px;
  border: 1px solid #333;
  border-radius: 5px;
}